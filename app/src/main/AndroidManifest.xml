<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pedrogomez.develepersfinder">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.pedrogomez.develepersfinder.HNMobAplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyListAplication">
        <activity
            android:name="com.pedrogomez.develepersfinder.splash.SplashActivity"
            android:screenOrientation="portrait"
            android:noHistory="true">
            <intent-filter>
                <action
                    android:name="android.intent.action.MAIN" />
                <category
                    android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.pedrogomez.develepersfinder.view.MainActivity"
            android:screenOrientation="portrait" />
    </application>

</manifest>